        var return_obj = {
            "id": id,
            "layout": "noBorders",
            "table": {
                "dontBreakRows": true,
                "headerRows": 0,
                "body": [
                    [{
                        "stack": stack_array
                    }]
                ]
            }
        };

        return return_obj;
    },

    _spacer: function(space) {

        space = space === undefined ? 10 : space;

