        if ((style !== "h1") && (style !== "h2") && (style !== "h3")) {
            style = "h2";
        };

        var id = "heading_" + style;


        var left = {
            "id": id,
            "stack": [{
                "text": text_left,
                "style": style,
                "margin": [0, 12, 0, 0],
                "alignment": "left"
            }],
            "margin": [0, 0, 0, 12]
