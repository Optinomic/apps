                        // Deferred when done.
                        actions_count = actions_count + 1;
                        if (dataService.checkSuccess(actions, actions_count)) {
                            deferred.resolve(return_data);
                        };
                    }, function(error) {
                        // Error
                        deferred.reject(return_data);
                        console.log('-- Error:', error);
                    });

                };
            } else {
