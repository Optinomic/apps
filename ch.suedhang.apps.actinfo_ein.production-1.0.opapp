            },
            _sr_actinfo_aus: {
                type: Object,
                statePath: function(state) {
                    if ((this.appIdAustritt !== null) && (this.appIdAustritt !== undefined)) {
                        if (this.appIdAustritt in state.survey_responses.data) {
                            return state.survey_responses.data[this.appIdAustritt]
                        } else {
                            return null
                        };
                    } else {
                        return null
                    };
                },
                observer: '_srChanged'
            },
            _patient: {
