                    };
                },
                observer: '_srChanged'
            },
            _patient: {
                type: Object,
                statePath: function(state) {
                    if ((state.patient.data !== null) && (state.patient.data !== undefined)) {
                        return state.patient.data;
                    } else {
                        return null
                    };
                }
            },
            _clinic: {
